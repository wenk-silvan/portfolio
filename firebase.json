{
  "hosting": {
    "public": "dist/wenk-portfolio/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/v1/**",
        "destination": "api"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "cors": [
      {
        "origin": ["http://localhost:4200", "https://silvan-wenk.com"],
        "method": ["GET"],
        "maxAgeSeconds": 3600
      }
    ]
  }
}
